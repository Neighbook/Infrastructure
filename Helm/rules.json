[
  {
    "id": "allow-anonymous-with-header-admin",
    "version": "v0.40.3",
    "upstream": {
      "preserve_host": false,
      "url": "http://kratos-admin.kratos-namespace.svc.cluster.local:80/"
    },
    "match": {
      "url": "<https|http>://demo.neighbook.tech/admin/<.*>",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ],
    "error": {
      "handler": "redirect",
      "config": {
        "to": "https://demo.neighbook.tech/auth/login"
      }
    }
  }
]